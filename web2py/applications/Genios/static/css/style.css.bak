/** #### BUTTONS ###### */

.btn {
   border-top: 1px solid #ffcb1f;
   background: #ffcb1f;
   background: -webkit-gradient(linear, left top, left bottom, from(#ffcb1f), to(#ffcb1f));
   background: -webkit-linear-gradient(top, #ffcb1f, #ffcb1f);
   background: -moz-linear-gradient(top, #ffcb1f, #ffcb1f);
   background: -ms-linear-gradient(top, #ffcb1f, #ffcb1f);
   background: -o-linear-gradient(top, #ffcb1f, #ffcb1f);
   padding: 6.5px 13px;
   -webkit-border-radius: 11px;
   -moz-border-radius: 11px;
   border-radius: 11px;
   -webkit-box-shadow: rgba(0,0,0,1) 0 1px 0;
   -moz-box-shadow: rgba(0,0,0,1) 0 1px 0;
   box-shadow: rgba(0,0,0,1) 0 1px 0;
   text-shadow: rgba(0,0,0,.4) 0 1px 0;
   color: white;
   font-size: 23px;
   font-family: Georgia, serif;
   text-decoration: none;
   vertical-align: middle;
   }
.btn:hover {
   border-top-color: #ffcb1f;
   background: #ffcb1f;
   color: #ccc;
   }
.btn:active {
   border-top-color: #fff34f;
   background: #fff34f;
   }
/** ##### Inputs #####*/

textarea, input[type="text"], input[type="password"], input[type="datetime"], input[type="datetime-local"], input[type="date"], input[type="month"], input[type="time"], input[type="week"], input[type="number"], input[type="email"], input[type="url"], input[type="search"], input[type="tel"], input[type="color"], .uneditable-input {

