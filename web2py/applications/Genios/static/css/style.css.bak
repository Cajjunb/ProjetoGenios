}


.menu_genios li{
    width: 150px;
    text-align: center;
    background:#fff;
    border-radius: 20px;
    margin-bottom: -40px;
    margin-right: 1px;
    line-height: 3;
    
}

.menu_genios li a{
    background: white;
    color: gray;
    font-size: 20px;
}

